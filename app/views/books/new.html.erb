<h1>新規投稿</h1>
<!-- form_with部分 -->
<%= form_with model: List.new do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
	
  <h4>本文</h4>
  <%= f.text_area :body %>
	
  <%= f.submit '投稿' %>
<% end %>


<h2>New book</h2>

<%= form_with model: @book, url: '/lists', method: :post  do |f| %>
  <div class="field">Title</div>
  <%= f.text_field :title %>
	
  <div class="field">Body</div>
  <%= f.text_area :body %>
  
	<div class="field"></div>
  <%= f.submit 'Create Book' %>
<% end %>  
